name: |
  <%= name %>

description: |
  <%= description %>

artist: <%= artist %>
artwork: <%= artwork %>
media_uri: <%= media_uri %>

uploads:
<% uploads.each do |upload| %>
- name: <%= upload.name %>
  host: <%= upload.host %>
  path: <%= upload.path %>
  user: <%= upload.user %>
  pass: <%= upload.pass %>

<% end %>

views:
<% views.each do |view| %>
- name: <%= view.name %>
  template: <%= view.template %>

<% end %>

informs:
<% informs.each do |inform| %>
- subject: |
    <%= inform.subject %>

  recipients:
<% inform.recipients.each do |recipient| %>
  - <%= recipient %>
<% end %>

  from: <%= inform.from %>
  template: <%= inform.template %>

<% end %>